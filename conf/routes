# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /products            controllers.ProductController.index

POST        /products            controllers.ProductController.search

GET         /products/new        controllers.ProductController.newProduct

POST        /products/new        controllers.ProductController.createProduct


# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)
